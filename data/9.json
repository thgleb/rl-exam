{
  "types": [
    "Все",
    "Неизменяемые приставки",
    "Приставки на -з и -с",
    "пра-//про-",
    "раз-(рас-)//роз-(рос-)",
    "пре-//при-"
  ],
  "data": [
    {
      "type": 1,
      "prefixes": [
        "в-", "об-", "от-", "над-", "под-", "у-",
        "о-", "вы-", "на-", "за-", "до-", "по-", "пере-",
        "c-"
      ]
    },
    {
      "type": 2,
      "prefixes": [
        "раз-//рас-", "из-//ис-", "без-//бес-", "воз-//вос-",
        "вз-//вс-", "низ-//нис-", "через-//черес-", "чрез-//чрес-"
      ],
      "rules": [
        "Если корень слова:",
        "— начинается на гласный или звонкий согласный — <b>з</b>",
        "— если корень начинается на глухой согласный — <b>c</b>"
      ]
    },
    {
      "type": 3,
      "prefixes": [
        "пра-", "про-"
      ],
      "rules": [
        "Если выражены значения:",
        "— давность, история, древность, отдаленность времен, явлений, отношений, лиц — <b>пра</b>",
        "— любые другие — <b>про</b>"
      ]
    },
    {
      "type": 4,
      "prefixes": [
        "раз- (рас-)",
        "роз- (рос-)"
      ],
      "rules": [
        "Если приставка:",
        "— без ударения — <b>a</b>",
        "— под ударением — <b>о</b>"
      ]
    },
    {
      "type": 5,
      "prefixes": [
        "пре-", "при-"
      ],
      "rules": [
        "<b>пре-</b>, если передаются значения:",
        "— «очень»",
        "— «пере»",
        "",
        "<b>при-</b>, если передаются значения:",
        "— «приближение»",
        "— «присоединение»",
        "— «неполнота действия»",
        "— «около, близко»"
      ]
    }
  ]
}